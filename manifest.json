{
    "manifest_version": 3,
    "name": "Tabs Tho",
    "version": "1.0",
    "description": "A Chrome extension with popup buttons",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Tabs Tho"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs"
    ]
}